---
import ProductHome from "@leaningtech/astro-theme/layouts/ProductHome.astro";
import LinkButton from "@leaningtech/astro-theme/components/LinkButton.astro";
import { Image } from "astro:assets";
import HeroDoodle from "../assets/robot_hero.png";
import Team from "../assets/team.png";
import KeyboardcatDoodle from "../assets/keyboard-cat.png";
import TicketDoodle from "../assets/ticket.png";
import DiverDoodle from "../assets/diver.png";
import WinnerDoodle from "../assets/winner.png";
import WizardDoodle from "../assets/wizard-cat.png";
import CalendarAsset from "../assets/calendar.png";
import { DISCORD_URL } from "@leaningtech/astro-theme/consts";
import Alessandro from "../assets/judges/alessandro.jpeg";
import Jules from "../assets/judges/jules.jpeg";
import Alex from "../assets/judges/alex.png";
import Mirko from "../assets/judges/mirko.jpeg";
import RuleBook from "../assets/rulebook.png";
import Directions from "../assets/direction.png";
---

<ProductHome
	style="w-full pt-8 bg-gradient-to-br from-black to-primary-500 via-[#0b0923]"
>
	<!-- Hero -->
	<section>
		<div class="lg:hidden justify-center px-32 py-8 sm:px-80 sm:pt-8">
			<Image src={HeroDoodle} alt="WebVM logo in a screen" class="" />
		</div>
		<div
			class="flex grid-cols-3 items-center px-4 pb-28 md:px-4 md:pt-4 lg:px-28 lg:pt-32 xl:pt-42 xl:px-28 2xl:px-48 3xl:px-602xl:pt-60"
		>
			<div
				class="col-span-2 mx-auto max-w-[56em] text-center space-y-4 sm:px-8"
			>
				<h1
					class="font-mono text-white text-3xl sm:text-4xl md:text-5xl lg:text-6xl xl:text-7xl font-semibold text-center lg:text-left text-wrap text-balance"
				>
					<span class="drop-shadow-[5px_0px_5px_rgba(240,125,153,0.9)]"
						>WebVM:</span
					>The hackathon II!
				</h1>
				<p
					class="font-mono text-xl md:text-3xl text-balance text-center lg:text-left text-[#befffa] drop-shadow-[0px_0px_5px_rgba(0,190,169,0.9)]"
				>
					Join the in-browser virtualization challenge!
				</p>
				<h1
					class="font-mono text-xl text-balance text-primary-300 text-center lg:text-left"
				>
					7 - 14 of April, 2025
				</h1>
				<p
					class="font-mono text-md text-wrap text-balance text-white text-center px-2 py-4 lg:text-left"
				>
					<b>WebVM: The hackathon II</b> is a global hybrid event open to all developers!
					Dive into the world of in-browser virtualization by leveraging <b
						>CheerpX, a WebAssembly-powered tool for executing x86 binaries on
						the browser.</b
					> Let your creativity flow and showcase your coding skills in this exciting
					event.
				</p>
				<div class="flex gap-4 justify-center lg:justify-start mt-12">
					<LinkButton
						type="primary-mono"
						href="https://webvm-hackathon.devpost.com/"
						label="Register"
						iconRight="ic:baseline-plus"
					/>
					<LinkButton
						type="secondary-mono"
						href="/docs"
						iconRight=""
						label="CheerpX docs"
					/>
				</div>
			</div>
			<div class="hidden lg:block flex justify-center 2xl:pl-32 2xl:px-16">
				<Image src={HeroDoodle} alt="WebVM logo in a screen" class="" />
			</div>
		</div>
	</section>

	<section
		id="guide"
		class="py-16 px-4 md:px-8 lg:px-28 xl:px-28 2xl:px-48 3xl:px-602 bg-black"
	>
		<div class="px-8">
			<h1
				class="font-mono font-extrabold text-4xl text-center text-white drop-shadow-[0px_0px_5px_rgba(0,190,169,0.6)] py-8"
			>
				In a nutshell
			</h1>
			<p
				class="font-mono text-lg sm:text-xl text-white text-pretty text-center"
			>
				Create a WebApp that integrates CheerpX following the theme: <b
					class="text-primary-300">Learning Linux on your Browser!</b
				>
				<b>The winner team gets a sweet 500 GBP gift card!</b>

				Happy coding! üöÄ.
			</p>

			<p
				class="font-mono text-lg sm:text-xl text-white text-pretty text-center py-10"
			>
				In the mean time take a look to the
				<a
					class="text-[#00b8a9] hover:underline"
					href="https://github.com/leaningtech/WebVMHackathon/blob/main/results2024.md"
				>
					2024 edition repository
				</a>
				.
			</p>
		</div>
	</section>
	<!-- Guide section -->
	<section
		class="py-16 px-4 md:px-8 lg:px-28 xl:px-28 2xl:px-48 3xl:px-602 bg-gradient-to-br from-[#0b0923] to-[#0b0923] via-black"
	>
		<h1
			class="font-mono font-extrabold text-4xl text-center text-white drop-shadow-[0px_0px_5px_rgba(240,125,153,0.6)] py-8"
		>
			How Does it all work?
		</h1>
		<!-- Step1: Find your squad -->
		<div class="sm:grid sm:grid-cols-5 py-16">
			<div class="col-span-2 flex justify-center items-center">
				<Image src={Team} alt="Cats in a box" class="max-w-96" />
			</div>
			<div
				id="team"
				class="col-span-3 text-center sm:text-left content-center px-8"
			>
				<h1
					class="font-bold text-xl md:text-2xl font-mono text-[#befffa] drop-shadow-[0px_0px_5px_rgba(0,190,169,0.9)]"
				>
					Step 1: Register your team
				</h1>
				<p class="font-mono text-sm sm:text-md 2xl:text-lg py-2">
					Gather your friends and colleagues, and get ready to dive into the
					challenge. <span class="italic font-bold"
						>Teams can range from 2 to 5 members.</span
					>
				</p>

				<p class="font-mono text-sm sm:text-md 2xl:text-lg pb-2">
					Are you a lone coder flying solo? No worries! Connect with potential
					team members in our <a
						target="_blank"
						class="text-primary-300 hover:underline"
						href="https://discord.com/channels/988743885121548329/1343955904067932171"
						>Discord server</a
					>, <a
						target="_blank"
						class="text-primary-300 hover:underline"
						href="https://webvm-hackathon.devpost.com/participants">DevPost</a
					> or on-site during the opening day!
				</p>
				<div class="flex gap-4 justify-center sm:justify-start mt-2">
					<LinkButton
						type="secondary-mono"
						href="https://webvm-hackathon.devpost.com/"
						label="Register"
						iconRight="mdi:register"
					/>
				</div>
			</div>
		</div>
		<!-- Step 2: Registration -->
		<div class="sm:grid grid-cols-5 py-6">
			<div
				class="sm:hidden col-span-2 flex justify-center items-center content-center"
			>
				<Image src={TicketDoodle} alt="cat typing" class="max-w-96" />
			</div>
			<div
				id="register"
				class="col-span-3 text-center sm:text-left content-center px-8"
			>
				<h1
					class="font-bold text-xl md:text-2xl font-mono text-[#befffa] drop-shadow-[0px_0px_5px_rgba(0,190,169,0.9)]"
				>
					Step 2: Secure your spot at the venue
				</h1>
				<p class="font-mono text-sm sm:text-md 2xl:text-lg py-2">
					This event offers a hybrid experience - you can fully engage online
					or, if you fancy, join us at one at our vibrant in-person location in
					Leeds, UK for the opening and closing days.
				</p>
				<p class="font-mono text-sm sm:text-md 2xl:text-lg pb-2">
					To snag your spot, each attendee should grab a free individual ticket
					on <a
						target="_blank"
						class="text-primary-300 hover:underline"
						href="https://www.eventbrite.co.uk/e/webvm-the-hackathon-ii-tickets-1277966595489"
						>Eventbrite</a
					>. Remember, space is limited, so if you reserve, be sure to show up!
				</p>
				<div class="flex gap-4 justify-center sm:justify-start mt-2">
					<LinkButton
						type="primary-mono"
						href="https://www.eventbrite.co.uk/e/webvm-the-hackathon-ii-tickets-1277966595489"
						label="Get tickets"
						iconRight="ion:ticket"
					/>
				</div>
			</div>
			<div
				class="hidden sm:block col-span-2 flex justify-center items-center content-center"
			>
				<Image src={TicketDoodle} alt="Ticket drawing" class="max-w-96" />
			</div>
		</div>
		<!-- Step 3:Dive -->
		<div class="sm:grid grid-cols-5 py-16">
			<div class="col-span-2 flex justify-center items-center content-center">
				<Image src={DiverDoodle} alt="Ticket" class="max-w-96" />
			</div>
			<div
				id="tickets"
				class="col-span-3 text-center sm:text-left content-center px-8"
			>
				<h1
					class="font-bold text-xl md:text-2xl font-mono text-[#befffa] drop-shadow-[0px_0px_5px_rgba(0,190,169,0.9)]"
				>
					Step 3: Dive into the hands-on week adventure!
				</h1>

				<p class="font-mono text-sm sm:text-md 2xl:text-lg pb-2">
					Whether you're joining us in-person or fully online, fear not! We've
					got your back with technical support through our <a
						target="_blank"
						class="text-primary-300 hover:underline"
						href="https://discord.com/channels/988743885121548329/1343955763944493168"
						>dedicated Discord channel</a
					>. Use this space to ask for help and connect with others.
				</p>
				<div class="flex gap-4 justify-center sm:justify-start mt-2">
					<LinkButton
						type="primary-mono"
						href={DISCORD_URL}
						label="Join Discord"
						iconRight="ic:baseline-discord"
					/>
				</div>
			</div>
		</div>
		<!-- Step 4: Submission and winner -->
		<div class="sm:grid grid-cols-5 py-16">
			<div class="sm:hidden col-span-2 flex justify-center items-center">
				<Image src={WinnerDoodle} alt="Winner" class="max-w-96" />
			</div>
			<div
				id="weekend"
				class="col-span-3 text-center sm:text-left content-center px-8"
			>
				<h1
					class="font-bold text-xl md:text-2xl font-mono text-[#befffa] drop-shadow-[0px_0px_5px_rgba(0,190,169,0.9)]"
				>
					Step 4: Submit your creation and await!
				</h1>
				<p class="font-mono text-sm sm:text-md 2xl:text-lg py-2">
					Submit your creation by the deadline (Monday 14th of April at 15:00 UK
					time) on <a
						target="_blank"
						class="text-primary-300 hover:underline"
						href="https://webvm-hackathon.devpost.com/"
						>DevPost (psss... make sure to meet the requirements and follow the
						rules!)</a
					>. Our panel of judges will review and score all the submissions.
					Await for the winner announcement on the 29th of April!
				</p>

				<div class="flex gap-4 justify-center sm:justify-start mt-2">
					<LinkButton
						type="secondary-mono"
						href="https://webvm-hackathon.devpost.com/"
						label="Submit"
						iconRight="mdi:send"
					/>
				</div>
			</div>
			<div
				class="hidden sm:block col-span-2 flex justify-center items-center content-center"
			>
				<Image src={WinnerDoodle} alt="Winner" class="max-w-96" />
			</div>
		</div>
		<div class="flex gap-4 justify-center items-center px-10 pt-4">
			<p class="font-mono text-center italic font-semibold">
				For more useful information and guidance, don't forget to read the docs
				or ask any technical questions on our Discord Server.
			</p>
		</div>
		<div class="flex gap-4 justify-center mt-12">
			<LinkButton
				type="primary-mono"
				href="https://discord.com/channels/988743885121548329/1283057350638571581"
				label="Support"
				iconRight="simple-icons:discord"
			/>
			<LinkButton
				type="secondary-mono"
				href="/docs"
				iconRight=""
				label="CheerpX docs"
			/>
		</div>
	</section>
	<section
		class="py-16 px-4 md:px-8 lg:px-28 xl:px-28 2xl:px-48 3xl:px-602 bg-black"
	>
		<h1
			class="font-mono font-extrabold text-4xl text-center text-white drop-shadow-[0px_0px_5px_rgba(240,125,153,0.6)] py-8"
		>
			Judging panel
		</h1>
		<div
			id="judges-grid"
			class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 space-x-2 font-mono text-center"
		>
			<div id="profile-container">
				<div id="picture" class="grid columns-1 justify-center py-8">
					<Image
						src={Alessandro}
						alt="Alessandro Pignotti"
						class="rounded-full"
					/>
					<a href="https://www.linkedin.com/in/alessandro-pignotti-10719617/">
						<p
							class="font-semibold text-lg text-[#befffa] drop-shadow-[0px_0px_5px_rgba(0,190,169,0.9)] hover:text-white pt-4"
						>
							Alessandro Pignotti
						</p>
					</a>
					<p class="text-xs text-white">Chief Technology Officer</p>
					<p class="italic text-xs text-white">Leaning Technologies</p>
				</div>
			</div>
			<div id="profile-container">
				<div id="picture" class="grid columns-1 justify-center py-8">
					<Image src={Alex} alt="Alex Bates" class="rounded-full" />
					<a href="https://www.linkedin.com/in/bates64/">
						<h2
							class="font-semibold text-lg text-[#befffa] drop-shadow-[0px_0px_5px_rgba(0,190,169,0.9)] hover:text-white pt-4"
						>
							Alex Bates
						</h2>
					</a>
					<p class="text-xs text-white">Graduate Software Engineer</p>
					<p class="italic text-xs text-white">Arm</p>
				</div>
			</div>
			<div id="profile-container">
				<div id="picture" class="grid columns-1 justify-center py-8">
					<Image
						src={Jules}
						alt="Jules Saarikoski"
						class="items-center rounded-full"
					/>
					<a href="https://www.linkedin.com/in/saarikoskijules/">
						<p
							class="font-semibold text-lg text-[#befffa] drop-shadow-[0px_0px_5px_rgba(0,190,169,0.9)] hover:text-white pt-4"
						>
							Jules Saarikoski
						</p>
					</a>
					<p class="text-xs text-white">Compilers Software Engineer</p>
					<p class="italic text-xs text-white">Leaning Technologies</p>
				</div>
			</div>
			<div id="profile-container">
				<div id="picture" class="grid columns-1 justify-center py-8">
					<Image src={Mirko} alt="Mirko Ferrati" class="rounded-full" />
					<a href="https://www.linkedin.com/in/mirkoferrati/">
						<p
							class="font-semibold text-lg text-[#befffa] drop-shadow-[0px_0px_5px_rgba(0,190,169,0.9)] hover:text-white pt-4"
						>
							Mirko Ferrati
						</p>
					</a>
					<p class="text-xs text-white">Software Engineering Manager</p>
					<p class="italic text-xs text-white">Canonical-Ubuntu</p>
				</div>
			</div>
		</div>
	</section>
	<!-- Section: Rules -->
	<section
		class="text-white py-16 px-4 md:px-8 lg:px-28 xl:px-28 2xl:px-48 3xl:px-602 bg-black"
	>
		<div class="flex justify-center items-center">
			<Image src={RuleBook} alt="Rule book" class="w-48 invert" />
		</div>
		<h1 class="font-mono font-bold text-4xl text-center py-8">
			Rules of the game
		</h1>
		<div class="flex gap-4 justify-center items-center px-10">
			<p class="font-mono italic font-semibold">
				Just like a perfectly executed code, a game isn't the same without its
				rules. <a
					class="text-primary-300 hover:underline"
					href="https://webvm-hackathon.devpost.com/rules"
					>Delve into these commandments (hey you, open this link!)</a
				> with the precision of a seasoned developer and make sure to stay looped
				in with them.
			</p>
		</div>
	</section>
	<!-- Agenda section -->
	<section
		id="agenda"
		class="text-white py-16 px-4 md:px-8 lg:px-28 xl:px-28 2xl:px-48 3xl:px-602 bg-black"
	>
		<div class="flex justify-center items-center">
			<Image src={CalendarAsset} alt="Calendar" class="w-48" />
		</div>
		<h1
			class="font-mono font-bold text-4xl text-center text-white drop-shadow-[0px_0px_5px_rgba(240,125,153,0.6)] pb-8"
		>
			Agenda
		</h1>
		<p class="font-mono text-center">
			Please consider this agenda is subject to change, stay tuned!
		</p>
		<div class="py-6">
			<h2
				class="font-mono text-md sm:text-lg font-bold text-[#befffa] drop-shadow-[0px_0px_5px_rgba(0,190,169,0.9)]"
			>
				Monday <span class="italic font-normal">7/april/2025</span>
			</h2>
			<div class="border border-black px-2">
				<div class="relative overflow-x-auto">
					<table
						class="min-w-96 w-full text-xs sm:text-sm text-left text-nowrap font-mono border border-transparent border-spacing-2"
					>
						<thead>
							<tr class="">
								<th class="pr-2 px-2">UK Time</th>
								<th class="pr-2 px-2">Event</th>
								<th class="pr-2 px-2">Leeds site</th>
								<th class="pr-2 px-2">Discord</th>
							</tr>
						</thead>
						<tbody>
							<tr class="even:bg-black odd:bg-teal-950">
								<td class="pr-2 px-2">10:30</td>
								<td class="pr-2 px-2">Reception</td>
								<td class="pr-2 px-2">Venue opens</td>
								<td class="pr-2 px-2">-</td>
							</tr>
							<tr class="even:bg-black odd:bg-teal-950">
								<td class="pr-2 px-2">11:00</td>
								<td class="pr-2 px-2">Welcome talk</td>
								<td class="pr-2 px-2">Main venue screen</td>
								<td class="pr-2 px-2"
									><a
										target="_blank"
										class="text-primary-300 hover:underline"
										href="https://discord.com/channels/988743885121548329/1131533743748825128"
										>Stream on Stage</a
									></td
								>
							</tr>
							<tr class="even:bg-black odd:bg-teal-950">
								<td class="pr-2 px-2">11:00</td>
								<td class="pr-2 px-2">Technical support starts</td>
								<td class="pr-2 px-2">-</td>
								<td class="pr-2 px-2"
									><a
										target="_blank"
										class="text-primary-300 hover:underline"
										href="https://discord.com/channels/988743885121548329/1343955763944493168"
										>#support channel</a
									></td
								>
							</tr>
							<tr class="even:bg-black odd:bg-teal-950">
								<td class="pr-2 px-2">11:30</td>
								<td class="pr-2 px-2">Team building</td>
								<td class="pr-2 px-2">In-person activity</td>
								<td class="pr-2 px-2"
									><a
										target="_blank"
										class="text-primary-300 hover:underline"
										href="https://discord.com/channels/988743885121548329/1343955904067932171"
										>#team-finder channel</a
									></td
								>
							</tr>
							<tr class="even:bg-black odd:bg-teal-950">
								<td class="pr-2 px-2">12:00</td>
								<td class="pr-2 px-2">Lunch break</td>
								<td class="pr-2 px-2">Refreshments provided</td>
								<td class="pr-2 px-2">-</td>
							</tr>
							<tr class="even:bg-black odd:bg-teal-950">
								<td class="pr-2 px-2">13:00</td>
								<td class="pr-2 px-2">Workshop: Intro to CheerpX</td>
								<td class="pr-2 px-2">Main venue screen</td>
								<td class="pr-2 px-2"
									><a
										target="_blank"
										class="text-primary-300 hover:underline"
										href="https://discord.com/channels/988743885121548329/1131533743748825128"
										>Stream on Stage</a
									></td
								>
							</tr>
							<tr class="even:bg-black odd:bg-teal-950">
								<td class="pr-2 px-2">14:00</td>
								<td class="pr-2 px-2">Independent work</td>
								<td class="pr-2 px-2">Teamwork at the venue</td>
								<td class="pr-2 px-2">Teamwork virtually</td>
							</tr>
							<tr class="even:bg-black odd:bg-teal-950">
								<td class="pr-2 px-2">16:00</td>
								<td class="pr-2 px-2">Technical support ends</td>
								<td class="pr-2 px-2">-</td>
								<td class="pr-2 px-2"
									><a
										target="_blank"
										class="text-primary-300 hover:underline"
										href="https://discord.com/channels/988743885121548329/1343955763944493168"
										>#support channel</a
									></td
								>
							</tr>
							<tr class="even:bg-black odd:bg-teal-950">
								<td class="pr-2 px-2">16:30</td>
								<td class="pr-2 px-2">Closing remarks</td>
								<td class="pr-2 px-2">Casual chat</td>
								<td class="pr-2 px-2"
									><a
										target="_blank"
										class="text-primary-300 hover:underline"
										href="https://discord.com/channels/988743885121548329/1343955468380405760"
										>#hackathon-announcements channel</a
									></td
